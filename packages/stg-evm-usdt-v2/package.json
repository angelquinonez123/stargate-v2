{
  "name": "@stargatefinance/usdt-evm",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "license": "ISC",
  "author": "",
  "exports": {
    "./deployed": {
      "types": "./dist/deployed/index.d.ts",
      "import": "./dist/deployed/index.mjs",
      "require": "./dist/deployed/index.cjs"
    },
    "./deployed/*": {
      "types": "./dist/deployed/*.d.ts",
      "import": "./dist/deployed/*.mjs",
      "require": "./dist/deployed/*.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "deployments"
  ],
  "scripts": {
    "build": "$npm_execpath build:exports && $npm_execpath build:ts",
    "build:exports": "npx @layerzerolabs/export-deployments -o deployed",
    "build:ts": "$npm_execpath tsc --noEmit && $npm_execpath tsup --clean",
    "clean": "rm -rf dist cache artifacts typechain-types node_modules",
    "compile": "hardhat compile",
    "test": "hardhat test"
  },
  "devDependencies": {
    "@layerzerolabs/export-deployments": "~0.0.14",
    "@layerzerolabs/io-devtools": "~0.1.13",
    "@layerzerolabs/typescript-config-next": "~2.3.25",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.8",
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomicfoundation/hardhat-verify": "^1.0.1",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-etherscan": "^3.1.7",
    "@openzeppelin/contracts": "4.2.0",
    "@openzeppelin/contracts-upgradeable": "4.2.0",
    "@openzeppelin/hardhat-upgrades": "^1.28.0",
    "@openzeppelin/upgrades-core": "^1.27.0",
    "@types/mocha": "~10.0.7",
    "chai": "^4.3.7",
    "dotenv": "^16.4.5",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.2",
    "hardhat": "^2.22.10",
    "hardhat-deploy": "^0.12.4",
    "hardhat-deploy-ethers": "^0.3.0-beta.13",
    "hardhat-gas-reporter": "^1.0.9",
    "solidity-coverage": "^0.8.12",
    "tsup": "^8.0.1",
    "typechain": "~8.3.2",
    "typescript": "~5.6.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
